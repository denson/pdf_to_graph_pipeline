      }
    },
    {
      "id": "Paper_Nalevankov\u00e1_et_al._-_2023_-_Comparative_Assessment_of_Sap_Flow_Modeling_Techni_abstract_method_4",
      "labels": [
        "Method"
      ],
      "properties": {
